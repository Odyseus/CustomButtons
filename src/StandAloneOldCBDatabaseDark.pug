doctype html
html
	head
		meta(http-equiv='content-type', content='text/html;charset=utf-8')
		title Custom Buttons By Odyseus
		link(rel='shortcut icon', type='image/x-icon', href='data:image/png;base64,')
		// meta(name='viewport', content='width=device-width, initial-scale=1')
		style(type='text/css')
			include ../lib/css/bootstrap-dark-for-standalone.css
		style(type='text/css')
			include ../lib/css/highlight-dark.css
		- // The next two files, if embeded, doesn't work.
		- // tweaks-common.css
		link(type='text/css', rel='stylesheet', href='data:text/css;base64,')
		- // tweaks-dark.css
		link(type='text/css', rel='stylesheet', href='data:text/css;base64,')

		script(type='text/javascript').
			var metadata = null;

		style(type='text/css').
			pre {
				color: #7b8a8b;
			}
			#old-buttons>li a:link {
				font-size: 1.25em !important;
				text-decoration: none !important;
			}
			#old-buttons img {
				border: 0 !important;
				margin: 0px 10px !important;
			}
			#old-buttons img {
				height: 18px !important;
				width: auto !important;
			}
			.glyphicon-chevron-up:before {
				content: url('data:image/png;base64,') !important;
			}

	body(role='document')

		include ./includes/_noscript.pug

		#mainarea

			span#headmenu
				include ./includes/_navbar_for_standalone_doc.pug

			.container.boxed
				h1 Buttons database
				p This is the old 
					a(href='https://addons.mozilla.org/firefox/addon/custom-buttons%C2%B2/') CB
						sup 2
					|  Forum Button Database from September 2010 compiled and published by 
					b morat
					| 
					a(href='http://custombuttons.sourceforge.net/forum/viewtopic.php?f=4&t=266') here
					| .
				.alert.alert-danger
					p
						b WARNING!!! Some of these buttons are very old and may not work at all or may even crash your browser. So take the proper precautions.
				div(align='middle')
					input(type='button', class='btn btn-primary btn-sm button-name' value='Show All Sources' onclick='self.toggleSp(this);')

				br
				include ./includes/_olddatabase.html

		include ./includes/_linktotop.pug

		include ./includes/_scriptsatbottom.pug
		script(type='text/javascript')
			include ../lib/js/jquery-2.2.4.min.js
		script(type='text/javascript')
			include ../lib/js/bootstrap.min.js
		script(type='text/javascript')
			include ../lib/js/highlight.pack.js
		script(type='text/javascript')
			include ../lib/js/myFunctions.js
		script(type='text/javascript')
			include ../lib/js/myFunctionsForOLDCBDatabase.js
