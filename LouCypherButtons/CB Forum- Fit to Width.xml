  <initcode><![CDATA[var imgOn  = "data:image/x-icon;base64,";
var imgOff = "data:image/x-icon;base64,";
this.type = "checkbox";

var uri = makeURI("data:text/css," + encodeURIComponent(this.Help));

var sss = Cc["@mozilla.org/content/style-sheet-service;1"].
          getService(Ci.nsIStyleSheetService);

if (sss.sheetRegistered(uri, sss.USER_SHEET)) {
  sss.unregisterSheet(uri, sss.USER_SHEET);
  this.image = imgOff;
  this.checked = false;
} else {
  sss.loadAndRegisterSheet(uri, sss.USER_SHEET);
  this.image = imgOn;
  this.checked = true;
}

// Reference: https://developer.mozilla.org/en/Using_the_Stylesheet_Service

]]></initcode>
