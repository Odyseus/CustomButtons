  <mode>0</mode>
  <initcode><![CDATA[let baseURL = "http://forums.mozillazine.org/"; 
let viewForum = baseURL + "viewforum.php?f=";
let image = this.image;

let mp = <menupopup xmlns={xulns}>
           <splitmenu label="Mozilla Firefox"
                      style={"list-style-image: url('" + image + "');"}
                      statustext={viewForum + "49"}
                      oncommand={"openUILink(this.parentNode.statusText, " +
                                 "event, false, true);"}
                      onclick="checkForMiddleClick(this, event);">
             <menupopup oncommand={"openUILink(event.target.statusText, " +
                                   "event, false, true);"}
                        onclick="checkForMiddleClick(this, event);"
                        onmouseover={"document.getElementById(" +
                                     "'statusbar-display').label=" +
                                     "event.target.statusText"}>
               <menuitem label="Firefox Support" class="menuitem-iconic"
                         image={image} statustext={viewForum + "38"}/>
               <menuitem label="Firefox General" class="menuitem-iconic"
                         image={image} statustext={viewForum + "7"}/>
               <menuitem label="Firefox Builds" class="menuitem-iconic"
                         image={image} statustext={viewForum + "23"}/>
               <menuitem label="Firefox Features" class="menuitem-iconic"
                         image={image} statustext={viewForum + "8"}/>
               <menuitem label="Firefox Bugs" class="menuitem-iconic"
                         image={image} statustext={viewForum + "9"}/>
             </menupopup>
           </splitmenu>

           <splitmenu label="Mozilla Thunderbird"
                      style={"list-style-image: url('" + image + "');"}
                      statustext={viewForum + "50"}
                      oncommand={"openUILink(this.parentNode.statusText, " +
                                 "event, false, true);"}
                      onclick="checkForMiddleClick(this, event);">
             <menupopup oncommand={"openUILink(event.target.statusText, " +
                                   "event, false, true);"}
                        onclick="checkForMiddleClick(this, event);"
                        onmouseover={"document.getElementById(" +
                                     "'statusbar-display').label=" +
                                     "event.target.statusText"}>
               <menuitem label="Thunderbird Support" class="menuitem-iconic"
                         image={image} statustext={viewForum + "39"}/>
               <menuitem label="Thunderbird General" class="menuitem-iconic"
                         image={image} statustext={viewForum + "28"}/>
               <menuitem label="Thunderbird Builds" class="menuitem-iconic"
                         image={image} statustext={viewForum + "29"}/>
               <menuitem label="Thunderbird Features" class="menuitem-iconic"
                         image={image} statustext={viewForum + "30"}/>
               <menuitem label="Thunderbird Bugs" class="menuitem-iconic"
                         image={image} statustext={viewForum + "31"}/>
             </menupopup>
           </splitmenu>

         </menupopup>;

this.type = "menu";
this.appendChild(cbu.makeXML(mp));

]]></initcode>
