  <initcode><![CDATA[var $ = this.AboutConfigMultipleSelection = {
	init: function() {
		if (!("Services" in window))
			Cu.import("resource://gre/modules/Services.jsm");

		// All the code using this it's "thanks" to SeaMonkey. ¬¬
		if (!($._UID in window)) {
			window[$._UID] = {
				Initialized: false,
			};
		}

		if (!window[$._UID]["Initialized"]) {
			window[$._UID]["Initialized"] = true;
			Services.obs.addObserver($.observer, "domwindowopened", false);
			window.addEventListener("DOMContentLoaded", $.listener, true);
		}

		addDestructor(function(aReason) {
			if (aReason === "update" && Services.appinfo.ID !== "{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}") {
				$.unInit();
			}
			if (aReason === "delete") {
				$.unInit();
			}
		});
	},

	unInit: function() {
		delete window[$._UID];
		Services.obs.removeObserver($.observer, "domwindowopened", false);
		window.removeEventListener("DOMContentLoaded", $.listener, true);
	},

	observer: {
		observe: function(aSubject, aTopic, aData) {
			aSubject.addEventListener("load", $.listener, true);
		},

		handleEvent: function(aE) {
			aE.target.defaultView.removeEventListener("load", $.listener, false);
		}
	},

	listener: function(aE) {
		aE.preventDefault();
		var doc = aE.target,
			win;
		if (!doc)
			return;
		if (!(doc.location.href.startsWith("chrome://global/content/config.xul") || doc.location.href.startsWith("about:config")))
			return;
		if (!doc.getElementById("configTree"))
			return;

		doc.getElementById("configTree").setAttribute("seltype", "multiple");
		win = doc.defaultView;
		var contextMenu = doc.getElementById("configContext");
		// Forced to use this check for SeaMonkey and its stupid multiple initialization.
		if (!win.document.getElementById($._UID + "copyAsFunction")) {
			let mItem = contextMenu.insertBefore(doc.createElement("menuitem"), doc.getElementById("copyValue").nextSibling);
			mItem.id = $._UID + "copyAsFunction";
			mItem.setAttribute("label", "Copy for user.js");
			mItem.setAttribute("accesskey", "u");
			mItem.setAttribute("oncommand", "copyAsFunction();");
		}

		win.getSelected = function() { // New function added
			var arr = [],
				i = 0,
				k = 0,
				iLen = win.view.selection.getRangeCount(),
				start = {},
				end = {};
			for (; i < iLen; i++) {
				win.view.selection.getRangeAt(i, start, end);
				for (k = start.value; k <= end.value; k++) {
					arr.push(win.gPrefView[k]);
				}
			}
			return arr;
		};

		win.ResetSelected = function() { // Overrides the default function
			win.getSelected().forEach(function(i) {
				Services.prefs.clearUserPref(i.prefCol);
			});
		};

		win.copyPref = function() { // Overrides the default function
			let arr = [];
			win.getSelected().forEach(function(i) {
				arr.push(i.prefCol + ";" + i.valueCol);
			});
			gClipboard.write(arr.join("\n"), document);
		};

		win.copyName = function() { // Overrides the default function
			let arr = [];
			win.getSelected().forEach(function(i) {
				arr.push(i.prefCol);
			});
			gClipboard.write(arr.join("\n"), document);
		};

		win.copyValue = function() { // Overrides the default function
			let arr = [];
			win.getSelected().forEach(function(i) {
				arr.push(i.valueCol);
			});
			gClipboard.write(arr.join("\n"), document);
		};

		win.copyAsFunction = function() { // New function added
			let arr = [];
			win.getSelected().forEach(function(i) {
				if (i.typeCol === 32) {
					arr.push('user_pref("' + i.prefCol + '", "' + i.valueCol + '");');
				} else {
					arr.push('user_pref("' + i.prefCol + '", ' + i.valueCol + ');');
				}
			});
			gClipboard.write(arr.join("\n"), document);
		};
		aE.stopPropagation();
	},
	_UID: "__about_config_multiple_selection_" + self.id.slice(20),
};

this.AboutConfigMultipleSelection.init();]]></initcode>
