	}
	.message[severity=info] > .icon {
		background-image: url("data:image/png;base64,") !important;
		background-repeat: no-repeat !important;
	}
	/* Assign new icons */

	/* Reposition new icons */
	.message > .icon {
		margin: 4px 0 0 4px !important;
	}
	/* Reposition new icons */

	/* Clear console icon */
	.webconsole-clear-console-button.devtools-toolbarbutton {
		list-style-image: url("data:image/png;base64,") !important;
		background-repeat: no-repeat !important;
